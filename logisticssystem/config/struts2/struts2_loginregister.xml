<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
	"http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="loginregister" namespace="/loginregister"
		extends="struts-default" strict-method-invocation="false">
		<action name="loginregister_*"
			class="com.logistics.loginregister.action.LoginRegisterAction"
			method="{1}">
			<result name="SuccessByUser">/WEB-INF/page/user/user-index.jsp</result>
			<result name="SuccessByStaff">/WEB-INF/page/admin/admin-index.jsp</result>
			<result name="pageReservationSuccess">/WEB-INF/page/express/reservation_success.jsp
			</result>
			<result name="pageReservationManager">/WEB-INF/page/express/reservation_manager.jsp
			</result>
			<result name="pageUnitList">/WEB-INF/page/unit/unit_list.jsp
			</result>
			<result name="pageRouteList">/WEB-INF/page/route/route_list.jsp
			</result>
			<result name="pageUnitAdd">/WEB-INF/page/unit/unit_add.jsp
			</result>
			<result name="pageStaffList">/WEB-INF/page/staff/staff_list.jsp
			</result>
			<result name="pageStaffAdd">/WEB-INF/page/staff/staff_add.jsp
			</result>
			<result name="pageRouteAdd">/WEB-INF/page/route/route_add.jsp
			</result>
			<result name="pageVehicleList">/WEB-INF/page/vehicle/vehicle_list.jsp
			</result>
			<result name="pageVehicleAdd">/WEB-INF/page/vehicle/vehicle_add.jsp
			</result>
			<result name="pageTeamManager">/WEB-INF/page/vehicle/team_manager.jsp
			</result>
			<result name="pageIndex" type="redirect">/user-login.jsp</result>
		</action>
	</package>
</struts>